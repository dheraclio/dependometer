<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE testmetrics SYSTEM "../testconfig.dtd">

<testmetrics> <!-- see dependometer-cpp for more examples -->
	<pagemetrics>
		<scanfile>project.html</scanfile>
		<metric>
			<name>number of project internal types</name>
			<value>4</value>
		</metric>
	</pagemetrics>
	
	<pagemetrics>

		<scanfile>type-metrics.html</scanfile>

		<!-- check type dependencies related to annotations -->
		<metric>
			<tablename>number of outgoing dependencies</tablename>
			<name>default.Invoker</name>
			<value>2</value>
		</metric>

		<metric>
			<tablename>number of outgoing dependencies</tablename>
			<name>default.ExceptionThrower</name>
			<value>2</value>
		</metric>

		<metric>
			<tablename>number of outgoing dependencies</tablename>
			<name>default.UncheckedException</name>
			<value>0</value>
		</metric>

		<metric>
			<tablename>number of outgoing dependencies</tablename>
			<name>default.CheckedException</name>
			<value>0</value>
		</metric>

	</pagemetrics>
	
</testmetrics>